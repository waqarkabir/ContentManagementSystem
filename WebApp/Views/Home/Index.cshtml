@model IEnumerable<WebApp.Models.Employee>

<html>
    <head>
        <title>

        </title>
    </head>
    <body>
        <h3>All Employees</h3>

        <table>
            <thead>
                <tr>
                    <td>Name</td>
                    <td>Email</td>
                    <td>Department</td>
                </tr>
            </thead>
            <tbody>
            @foreach (var item in Model)
            {
             <tr>
                 <td>@item.Name</td>
                 <td>@item.Email</td>
                 <td>@item.Department</td>
             </tr>   
            }
        </tbody>
        </table>
    </body>
</html>